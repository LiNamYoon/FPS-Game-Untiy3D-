// 사용 기술 : Application.LoadLevel(1)/   Application.Quit()

using UnityEngine;
using System.Collections;

namespace S3
{
    public class MainMenu : MonoBehaviour
    {

        public void PlayButton()
        {
            Application.LoadLevel(1);
        }
        public void ExitButton()
        {
            Application.Quit();
        }


    }

}
